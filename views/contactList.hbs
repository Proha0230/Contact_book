<div class="mainTableContactList">
    <h1>Список контактов</h1>
    {{#if contact.length}}
    <div class="tableContactList"> 
        <table>
            <thead>
                <tr>
                    <th>Номер</th>
                    <th>Ф.И.О</th>
                    <th>Дата Рождения</th>
                    <th>Действия</th>
                </tr>
            </thead>
            <tbody>
                {{#each contact}}
                <tr>
                    <td>{{phone}}</td>
                    <td>{{fio}}</td>
                    <td>{{dob}}</td>
                    <td class="tableContactButtonList">                      
                        <a href="/contactInfo/{{id}}">Открыть</a>
                    </td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
    {{else}}
    <h2>Контактов пока нет</h2>
    {{/if}}
</div>